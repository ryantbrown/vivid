{
  "name": "vivid",
  "version": "0.0.1",
  "description": "Active record based ORM for Node, supporting PostgresSQL, MySQL and Sqlite3",
  "main": "lib/vivid.js",
  "scripts": {
    "build": "webpack --mode=build",
    "dev": "webpack --progress --colors --watch --mode=dev",
    "test": "mocha --compilers js:babel-core/register --colors --check-leaks -t 5000 -b -R spec test/index.js",
    "cover": "babel-node ./node_modules/istanbul/lib/cli.js --config=./test/.istanbul.yml cover ./node_modules/mocha/bin/_mocha ./test/index.js"
  },
  "dependencies": {
    "bluebird": "^3.4.0",
    "inflection": "^1.10.0",
    "knex": "^0.11.5"
  },
  "devDependencies": {
    "babel": "6.3.13",
    "babel-cli": "^6.9.0",
    "babel-core": "6.1.18",
    "babel-eslint": "5.0.0",
    "babel-loader": "6.1.0",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-preset-es2015": "6.3.13",
    "chai": "3.4.1",
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "eslint": "1.7.2",
    "eslint-loader": "1.1.0",
    "interpret": "^1.0.1",
    "istanbul": "^0.4.3",
    "json-loader": "^0.5.4",
    "liftoff": "^2.2.1",
    "minimist": "^1.2.0",
    "mocha": "2.3.4",
    "pg": "^4.5.5",
    "sinon": "^1.17.4",
    "tildify": "^1.2.0",
    "webpack": "1.12.9",
    "yargs": "3.32.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ryantbrown/vivid.git"
  },
  "keywords": [
    "orm"
  ],
  "author": "Ryan Brown",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ryantbrown/vivid.git/issues"
  },
  "homepage": "https://github.com/ryantbrown/vivid.git"
}
